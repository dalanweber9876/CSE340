<% if (title) { %>
    <h1><%= title %></h1>
    <p><%- messages() %></p>
    <% if (errors) { %>
        <ul class="notice">
       <% errors.array().forEach(error => { %>
         <li><%= error.msg %></li>
      <%  }) %>
       </ul>
      <% } %>

    <h2>You're logged in!</h2>

    <% } else {
      res.redirect('/')
    } %>

    

