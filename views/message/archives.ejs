<% if (title) { %>
    <h1 class="management-title"><%= title %></h1>
    <p class="management-messages"><%- messages() %></p>
    <% if (errors) { %>
        <ul class="notice">
       <% errors.array().forEach(error => { %>
         <li><%= error.msg %></li>
      <%  }) %>
       </ul>
      <% } %>
      <a href="/inbox">Return to inbox</a>

    <%- messageTable %>

<% } else {
    res.redirect('/')
    } %>